{
  "name": "electron-quick-start",
  "version": "4.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "build": {
    "appId": "com.demo.app",
    "mac": {
      "target": ["dmg", "zip"],
      "icon": "icon.icns"
    },
    "win": {
      "target": ["nsis", "zip"],
      "icon": "256.ico"
    },
    "directories": {
      "output": "out"
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "repository": "https://github.com/xiangqingyi/electron-update-demo",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "electron-log": "^3.0.8",
    "electron-updater": "^4.1.2",
    "update-electron-app": "^1.5.0"
  }
}
